{
    "name": "catfishphp/container",
    "description": "Lightweight DI container",
    "type": "library",
    "require": {
        "php": "^7.1.3",
        "psr/container": "^1.0",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "squizlabs/php_codesniffer": "3.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Catfish\\Container\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "sniff": "vendor/bin/phpcs --standard=.phpcs.xml -n --extensions=php src tests"
    }
}
